//Tema1
var matricula:numerico//costo de la matricula
var c1,c2,c3,c4,c5:numerico//calificaciones

inicio 
		cls()
		imprimir("Ingrese la matricula del alumno\n")
		leer(matricula)
	
		imprimir("Ingrese las 5 calificaciones del alumno\n")
		leer(c1,c2,c3,c4,c5)

		imprimir("La matricula es ",matricula," y el promedio del alumno es ",(c1+c2+c3+c4+c5)/5)







fin
