
var N:numerico

	inicio
		
		cls()
		imprimir("Ingrese un numero mayor que 0\n")
		leer(N)
		
		mientras(N<=0){
			
			imprimir("Ingrese un numero mayor que 0\n")
			leer(N)

			}
			si(N%2==0){
				imprimir(N," es par valor= ",2*N)


		
			si(N%4==0){
				imprimir("\n",N," es multiplo de 4 valor ",N/2)
							}

					
			sino
				imprimir("N= ",N," valor= ",10/N)			


				}

		
		









	fin